{
  "name": "launcher.app.privacysafe.io",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build:deno": "deno bundle './src-deno/settings-deno.ts' -- './app/settingsDenoServices.js'",
    "build:installer": "tsc -p src-installer/tsconfig.json || echo '\n⚠️ ignoring error code'",
    "build": "vue-tsc --noEmit && vite build && npm run build:deno && npm run build:installer",
    "serve": "vite preview",
    "stylelint": "stylelint --config stylelint.config.js 'src/**/*.{vue,scss}'",
    "stylelint:fix": "stylelint --config stylelint.config.js --fix 'src/**/*.{vue,scss}'",
    "linter": "eslint -c .eslintrc.js --ext .js,.vue,.ts, src",
    "linter:fix": "eslint -c .eslintrc.js --fix --ext .js,.vue,.ts, src",
    "lint": "npm run linter && npm run stylelint"
  },
  "dependencies": {
    "@iconify-icons/ic": "^1.2.13",
    "@varlet/touch-emulator": "^2.13.0",
    "@varlet/ui": "^2.13.0",
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-router": "4.2.4"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/vue": "^7.0.0",
    "@types/jasmine": "^4.3.5",
    "@types/jest": "^29.5.3",
    "@types/node": "^16.18.38",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/devtools": "^6.5.0",
    "@vuedx/typescript-plugin-vue": "^0.7.6",
    "eslint": "^8.45.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.15.1",
    "jest": "^29.6.1",
    "jest-serializer-vue": "^3.1.0",
    "jest-transform-stub": "^2.0.0",
    "postcss-html": "^1.5.0",
    "sass": "^1.63.6",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-rscss": "^0.4.0",
    "stylelint-scss": "^4.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.4",
    "vue-jest": "^3.0.7",
    "vue-tsc": "^1.8.5"
  }
}
