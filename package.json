{
  "name": "launcher.app.privacysafe.io",
  "version": "0.9.11",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean:dist": "rm -rf app/",
    "copy:settings": "mkdir -p app/constants && cp -r src/common/constants/settings.json app/constants/settings.json",
    "build": "pnpm run clean:dist && vue-tsc --noEmit && vite build && pnpm run copy:settings",
    "serve": "vite preview",
    "stylelint": "stylelint --config .stylelint.config.cjs 'src/**/*.{vue,scss}'",
    "stylelint:fix": "stylelint --config .stylelint.config.cjs --fix 'src/**/*.{vue,scss}'",
    "linter": "eslint -c eslint.config.js --ext .vue,.ts, src",
    "linter:fix": "eslint -c eslint.config.js --fix --ext .vue,.ts, src",
    "lint": "pnpm run linter && pnpm run stylelint"
  },
  "dependencies": {
    "@types/semver": "^7.7.0",
    "@v1nt1248/3nclient-lib": "0.2.7",
    "lodash": "4.17.21",
    "pinia": "3.0.2",
    "semver": "^7.7.2",
    "vue": "3.5.15",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/vue": "8.1.0",
    "@types/jasmine": "5.1.8",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.17",
    "@types/node": "20.17.51",
    "@typescript-eslint/eslint-plugin": "8.33.0",
    "@typescript-eslint/parser": "8.33.0",
    "@vitejs/plugin-vue": "5.2.4",
    "@vue/devtools": "7.7.6",
    "@vue/eslint-config-typescript": "14.5.0",
    "@vuedx/typescript-plugin-vue": "0.7.6",
    "eslint": "9.27.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-vue": "9.33.0",
    "globals": "^15.15.0",
    "jest": "29.7.0",
    "jest-serializer-vue": "3.1.0",
    "jest-transform-stub": "2.0.0",
    "postcss-html": "1.8.0",
    "prettier": "^3.5.3",
    "sass": "1.89.0",
    "stylelint": "16.19.1",
    "stylelint-config-recommended-scss": "14.1.0",
    "stylelint-config-recommended-vue": "1.6.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-scss": "6.12.0",
    "ts-jest": "29.3.4",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.33.0",
    "unplugin-vue-components": "0.28.0",
    "vite": "6.3.5",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vue-eslint-parser": "^10.1.3",
    "vue-jest": "3.0.7",
    "vue-tsc": "2.2.10"
  },
  "packageManager": "pnpm@10.10.0"
}
