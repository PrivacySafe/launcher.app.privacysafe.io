{
  "name": "launcher.app.privacysafe.io",
  "version": "0.11.5",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean:dist": "rm -rf app/",
    "copy:settings": "mkdir -p app/constants && cp -r src/common/constants/settings.json app/constants/settings.json",
    "build": "pnpm run clean:dist && vue-tsc --noEmit && vite build && pnpm run copy:settings",
    "tests:build": "bash tests-app/compile-and-build.sh",
    "tests:build-all": "pnpm run build && pnpm run tests:build",
    "tests:run-on": "bash tests-app/run-tests-on.sh",
    "serve": "vite preview",
    "stylelint": "stylelint --config .stylelint.config.cjs 'src/**/*.{vue,scss}'",
    "stylelint:fix": "stylelint --config .stylelint.config.cjs --fix 'src/**/*.{vue,scss}'",
    "linter": "eslint -c eslint.config.js --ext .vue,.ts, src",
    "linter:fix": "eslint -c eslint.config.js --fix --ext .vue,.ts, src",
    "lint": "pnpm run linter && pnpm run stylelint"
  },
  "dependencies": {
    "@terrastruct/d2": "^0.1.33",
    "@types/semver": "^7.7.1",
    "@v1nt1248/3nclient-lib": "0.2.46",
    "lodash": "4.17.21",
    "pinia": "3.0.3",
    "semver": "^7.7.2",
    "vue": "3.5.21",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/vue": "8.1.0",
    "@types/jasmine": "5.1.9",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.20",
    "@types/node": "22.18.3",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/devtools": "7.7.7",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vuedx/typescript-plugin-vue": "0.7.6",
    "eslint": "9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-vue": "10.4.0",
    "globals": "^16.4.0",
    "jest": "30.1.3",
    "jest-serializer-vue": "3.1.0",
    "jest-transform-stub": "2.0.0",
    "postcss-html": "1.8.0",
    "prettier": "^3.6.2",
    "sass": "1.92.1",
    "stylelint": "16.24.0",
    "stylelint-config-recommended-scss": "16.0.1",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard": "39.0.0",
    "stylelint-scss": "6.12.1",
    "ts-jest": "29.4.1",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "7.1.5",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-eslint-parser": "^10.2.0",
    "vue-jest": "3.0.7",
    "vue-tsc": "3.0.7"
  },
  "packageManager": "pnpm@10.16.0"
}
